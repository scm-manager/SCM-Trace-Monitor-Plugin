  "permissions": {
    "configuration": {
      "read,write": {
        "traceMonitor": {
          "displayName": "Modify global trace monitor configuration",
          "description": "May modify the global trace monitor configuration"
        }
      }
    },
    "traceMonitor": {
      "read": {
        "displayName": "Access trace monitor",
        "description": "May see the trace monitor"
      }
    }
  }
