{
  "scm-trace-monitor-plugin": {
    "navLink": "Trace Monitor",
    "global": {
      "title": "Trace Monitor Configuration",
      "nav-link": "Trace Monitor"
    },
    "config": {
      "form": {
        "storeSize": "Amount of stored requests per category",
        "storeSizeHelpText": "Defines how many requests will be stored per category. \nBe careful: Increasing the store size too much could cause high memory usage."
      }
    },
    "table": {
      "column": {
        "kind": "Category",
        "status": "Result",
        "timestamp": "Timestamp",
        "duration": "Duration"
      },
      "success": "success",
      "failed": "failed",
      "details": "Show details"
    }
